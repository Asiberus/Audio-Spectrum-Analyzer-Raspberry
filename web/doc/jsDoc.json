{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": ["./"],
    "exclude": ["./dist/", "./doc/", "./lib/", "./node_modules/", "./webpack/"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "name": "Light Fever 440",
    "footerText": "Light Fever 440 - Version 0.0.1",
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "node_modules/tui-jsdoc-template",
    "destination": "./doc/",
    "readme": "../README.md",
    "package": "./package.json",
    "access": "all",
    "recurse": true,
    "verbose": true,
    "private": true
  }
}
